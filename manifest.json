{
  "manifest_version": 2,
  "name": "Facebook timekeeper",
  "version": "0.1",
  "description": "keeps info on your time spent on Facebook",

  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": ["js/jquery.js", "js/background.js"]
  },
  "browser_action": {}
}
