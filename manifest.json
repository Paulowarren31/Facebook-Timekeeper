{
  "manifest_version": 2,
  "name": "Facebook timekeeper",
  "version": "0.1",
  "description": "keeps info on your time spent on Facebook",
  "content_scripts": [{
    "matches": ["*://www.facebook.com/*"],
    "js": ["js/jquery.js", "js/profile.js"]
  }],

  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "webRequest"
  ],
  "background": {
    "scripts": ["js/jquery.js", "js/background.js"]
  },
  "browser_action": {}
}
